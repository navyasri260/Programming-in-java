import java.util.*;
public class reverse string{
   public static void main(string[] args){
       string str="temple";
       string rev=" ";
       for(int i=str length()-1;i>=0;i--){
           rev+=str.charAT(i);
       }
       system.out.println("string:"+str);
       system.out.println("reverse:"+rev);
   }
}

